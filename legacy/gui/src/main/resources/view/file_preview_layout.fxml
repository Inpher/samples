            </Label>
            </children>
            <padding>
                <Insets bottom="30.0" top="30.0" />
            </padding>
        </GridPane>
        <HBox alignment="CENTER" VBox.vgrow="NEVER">
            <children>
                <Button fx:id="openButton" mnemonicParsing="false" text="Open" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="50.0" left="30.0" right="30.0" top="60.0" />
    </padding>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</VBox>
